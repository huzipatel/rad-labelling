# Production Environment Variables
# Copy this to .env.production and fill in the values

# Database (use managed PostgreSQL with PostGIS)
DATABASE_URL=postgresql://user:password@host:5432/labelling_db

# Security
JWT_SECRET_KEY=your-secure-random-key-here

# Google APIs
GSV_API_KEY=your-google-street-view-api-key
GOOGLE_CLIENT_ID=your-google-oauth-client-id
GOOGLE_CLIENT_SECRET=your-google-oauth-client-secret

# Google Cloud Storage
GCS_BUCKET_NAME=your-gcs-bucket-name
GCS_CREDENTIALS_PATH=/app/credentials/gcs-credentials.json

# Redis (if using external Redis)
REDIS_URL=redis://localhost:6379/0

# Frontend
VITE_API_URL=/api/v1
VITE_GSV_API_KEY=your-google-maps-api-key

# Ports (optional)
BACKEND_PORT=8000
FRONTEND_PORT=3000




